   "source": [
    "hogar_df = pd.read_csv('csv/familias.csv', sep=';', encoding='Windows-1252')\n",
    "\n",
    "hogar_df.rename(columns={'Municipios (con más de 50.000 habitantes) y capitales de provincia':'Municipio',\n",
    "                                \"Año de construcción del edificio\" : \"Año de construcción\"\n",
    "}, inplace=True)\n",
    "\n",
    "hogar_df['Municipio'] = hogar_df['Municipio'].str.split(' ', n=1).str[1]\n",
    "\n",
    "hogar_pivot_df = hogar_df.pivot(index='Municipio', columns=['Tamaño del hogar'], values='Total')\n",
    "hogar_pivot_df.reset_index(inplace=True)\n",
    "\n",
    "hogar_pivot_df['Municipio'] = hogar_pivot_df['Municipio'].replace(Correciones)\n",
    "\n",
    "hogar_pivot_df.reset_index()\n",
    "\n",
    "hogar_pivot_20_df = hogar_pivot_df[hogar_pivot_df['Municipio'].isin(municipios_top_20)]\n",
    "\n",
    "hogar_pivot_20_df.drop(\"Total (tamaño del hogar)\", axis=1, inplace=True)\n",
    "\n",
    "hogar_pivot_20_df.reset_index(inplace=True, drop=True)\n",
    "\n",
    "hogar_pivot_20_df.columns.names = ['Index']\n",
    "\n",
    "hogar_pivot_20_df.to_csv(\"data/Tamaño del hogar\", sep=';', encoding='utf-8')"
   ]

    "Turismo_20_df = Turismo_df[Turismo_df['Municipio'].isin(municipios_top_20)]\n",
    "\n",
    "Turismo_20_df = Turismo_20_df[(Turismo_20_df['id'].astype(int) > 53)]\n",
    "\n",
    "Turismo_20_df.reset_index(inplace=True, drop=True)\n",
    "\n",
    "#Dejar solo la columna final\n",

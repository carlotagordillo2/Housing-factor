   "source": [
    "construccion_df = pd.read_csv('csv/año_construccion.csv', sep=';', encoding='Windows-1252')\n",
    "\n",
    "construccion_df.rename(columns={'Municipios (con más de 50.000 habitantes) y capitales de provincia':'Municipio',\n",
    "                                \"Año de construcción del edificio\" : \"Año de construcción\"\n",
    "}, inplace=True)\n",
    "\n",
    "construccion_df['Municipio'] = construccion_df['Municipio'].str.split(' ', n=1).str[1]\n",
    "\n",
    "construccion_pivot_df = construccion_df.pivot(index='Municipio', columns=['Tipo de vivienda (principal o no)', \"Año de construcción\"], values='Total')\n",
    "construccion_pivot_df.reset_index(inplace=True)\n",
    "\n",
    "construccion_pivot_df.columns = [' '.join(map(str, col)).strip() for col in construccion_pivot_df.columns.values]\n",
    "\n",
    "\n",
    "construccion_pivot_df['Municipio'] = construccion_pivot_df['Municipio'].replace(Correciones)\n",
    "\n",
    "construccion_pivot_20_df = construccion_pivot_df[construccion_pivot_df['Municipio'].isin(municipios_top_20)]\n",
    "\n",
    "construccion_pivot_20_df.reset_index(inplace=True, drop=True)\n",
    "\n",
    "construccion_pivot_20_df.to_csv(\"data/Año de construcción\", sep=';', encoding='utf-8')\n",
    "\n",
    "\n",
    "\n"
   ]
